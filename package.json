  "scripts": {
    "prepublishOnly": "yarn lint && yarn test:types && yarn clean:builds && yarn build",
    "check:dependencies": "yarn outdated -depth 0 --long",
    "lint": "yarn eslint ./src --format=pretty",
    "lint:fix": "yarn lint --fix --quiet",
    "test": "yarn test:types && yarn test:js",
    "test:types": "yarn ttsc --noEmit",
    "test:js": "yarn cross-env NODE_ENV=test TZ=Europe/Berlin jest --runInBand",
    "clean:dependencies": "yarn rimraf ./node_modules",
    "clean:builds": "yarn rimraf ./dist",
    "build": "yarn build:types && yarn build:js && yarn build:docs",
    "build:types": "yarn ttsc --emitDeclarationOnly",
    "build:js": "yarn rollup --config ./rollup.config.js",
    "build:docs": "yarn build-storybook -c .storybook -o ./docs",
    "watch:build": "yarn build:js --watch",
    "watch:test": "yarn test:js --watch",
    "start": "yarn start:storybook",
    "start:storybook": "yarn start-storybook --port ${PORT:-10000}",
    "start:docs": "yarn serve --listen ${PORT:-10000} ./docs"
